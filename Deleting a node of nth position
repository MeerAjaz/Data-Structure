#include<stdio.h>
#include<stdlib.h>
void insert(int); //declaring the prototype of functions.
void print();
void delete(int);
typedef struct node
{
    int data;
    struct node *next;
}node;
node *head;
int main()
{
    head = NULL;
    insert(2);//inserting elements in the list.
    insert(3);
    insert(4);
    insert(5);
    print();
    int n;
    printf("Enter the postion of node that u want to delete:");
    scanf("%d",&n);
    delete(n);
    print();
}
void insert(int x)
{
  node *temp1 = malloc(sizeof(node));
  temp1->data = x;
  temp1->next = NULL;
  if(head == NULL)
  {
      head = temp1;
      return;
  }
  node *temp2 = head;
  while(temp2->next != NULL) //traversing the list.
  {
      temp2 = temp2->next;
  }
  temp2->next = temp1;
 // free(temp2);
}
void print()
{
  node *temp = head;
  printf("The list is: ");
  while(temp != NULL)
  {
    printf("%d ",temp->data);
    temp = temp->next;
  }
  printf("\n");
}
void delete(int n)
{
    node *temp1 = head;
    if(n == 1) //for first node.
    {
      head = head->next;
      free(temp1);
      return;
    }
    for(int i=0; i<n-2; i++)//for remaining nodes.
    {
      temp1 = temp1->next;
    }
    node *temp2 = temp1->next->next;
    temp1->next = temp2;
}
